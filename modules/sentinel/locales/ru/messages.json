{
    "$schema": "../../../../.schema/i18n-schema.json",
    "language": "ru",
    "messages": [
        {"id": "nav.sentinel", "message": "Sentinel", "translation": "Sentinel"},
        {"id": "sentinel.title", "message": "Sentinel - IP Security", "translation": "Sentinel - Безопасность IP"},
        {"id": "sentinel.description", "message": "Manage IP banning, auto-ban paths, and whitelist", "translation": "Управление блокировкой IP, автобаном по путям и белым списком"},

        {"id": "sentinel.whitelist_title", "message": "Whitelisted IPs", "translation": "Белый список IP"},
        {"id": "sentinel.whitelist_description", "message": "IPs that bypass all security checks", "translation": "IP-адреса, которые пропускают все проверки безопасности"},
        {"id": "sentinel.whitelist_notes_placeholder", "message": "Description (optional)", "translation": "Описание (опционально)"},
        {"id": "sentinel.add_whitelist", "message": "Add to Whitelist", "translation": "Добавить в белый список"},
        {"id": "sentinel.remove_whitelist", "message": "Remove from whitelist", "translation": "Удалить из белого списка"},
        {"id": "sentinel.no_whitelist", "message": "No whitelisted IPs", "translation": "Белый список пуст"},
        {"id": "sentinel.success_whitelist_created", "message": "IP added to whitelist", "translation": "IP добавлен в белый список"},
        {"id": "sentinel.error_whitelist_duplicate", "message": "This IP is already whitelisted", "translation": "Этот IP уже в белом списке"},
        {"id": "sentinel.error_whitelist_create_failed", "message": "Failed to add to whitelist", "translation": "Не удалось добавить в белый список"},
        {"id": "sentinel.confirm_delete_whitelist", "message": "Remove this IP from the whitelist?", "translation": "Удалить этот IP из белого списка?"},

        {"id": "sentinel.paths_title", "message": "Auto-Ban Paths", "translation": "Автобан по путям"},
        {"id": "sentinel.paths_description", "message": "IPs accessing these paths will be automatically banned", "translation": "IP-адреса, обращающиеся к этим путям, будут автоматически заблокированы"},
        {"id": "sentinel.path_pattern_placeholder", "message": "e.g., /wp-admin, /.env, */phpMyAdmin*", "translation": "напр., /wp-admin, /.env, */phpMyAdmin*"},
        {"id": "sentinel.path_pattern_help", "message": "Patterns: /path (exact), /path* (starts with), */path (ends with), */path* (contains)", "translation": "Шаблоны: /path (точно), /path* (начинается с), */path (заканчивается на), */path* (содержит)"},
        {"id": "sentinel.path_notes_placeholder", "message": "Description (optional)", "translation": "Описание (опционально)"},
        {"id": "sentinel.add_path", "message": "Add Path", "translation": "Добавить путь"},
        {"id": "sentinel.remove_path", "message": "Remove path", "translation": "Удалить путь"},
        {"id": "sentinel.table_path", "message": "Path Pattern", "translation": "Шаблон пути"},
        {"id": "sentinel.no_paths", "message": "No auto-ban paths configured", "translation": "Пути для автобана не настроены"},
        {"id": "sentinel.success_path_created", "message": "Auto-ban path added", "translation": "Путь для автобана добавлен"},
        {"id": "sentinel.error_path_required", "message": "Path pattern is required", "translation": "Шаблон пути обязателен"},
        {"id": "sentinel.error_invalid_path_pattern", "message": "Invalid path pattern format", "translation": "Неверный формат шаблона пути"},
        {"id": "sentinel.error_path_duplicate", "message": "This path pattern already exists", "translation": "Этот шаблон пути уже существует"},
        {"id": "sentinel.error_path_create_failed", "message": "Failed to create path", "translation": "Не удалось создать путь"},
        {"id": "sentinel.confirm_delete_path", "message": "Remove this auto-ban path?", "translation": "Удалить этот путь для автобана?"},

        {"id": "sentinel.banned_title", "message": "Banned IPs", "translation": "Заблокированные IP"},
        {"id": "sentinel.banned_description", "message": "Manually banned IPs and auto-banned IPs from path triggers", "translation": "Вручную заблокированные IP и автоматически заблокированные по путям"},
        {"id": "sentinel.ip_pattern", "message": "IP Pattern", "translation": "IP-адрес"},
        {"id": "sentinel.ip_pattern_placeholder", "message": "e.g., 192.168.1.100 or 192.168.1.*", "translation": "напр., 192.168.1.100 или 192.168.1.*"},
        {"id": "sentinel.ip_pattern_help", "message": "Use * as wildcard: 192.168.1.* blocks all IPs starting with 192.168.1.", "translation": "Используйте * как маску: 192.168.1.* блокирует все IP, начинающиеся с 192.168.1."},
        {"id": "sentinel.notes", "message": "Notes", "translation": "Заметки"},
        {"id": "sentinel.notes_placeholder", "message": "Reason for ban (optional)", "translation": "Причина блокировки (опционально)"},
        {"id": "sentinel.url", "message": "URL", "translation": "URL"},
        {"id": "sentinel.url_placeholder", "message": "Related URL (optional)", "translation": "Связанный URL (опционально)"},
        {"id": "sentinel.ban_ip", "message": "Ban IP", "translation": "Заблокировать"},
        {"id": "sentinel.remove_ban", "message": "Remove ban", "translation": "Снять блокировку"},
        {"id": "sentinel.table_ip", "message": "IP Pattern", "translation": "IP-адрес"},
        {"id": "sentinel.table_notes", "message": "Notes", "translation": "Заметки"},
        {"id": "sentinel.table_url", "message": "URL", "translation": "URL"},
        {"id": "sentinel.table_banned_at", "message": "Banned At", "translation": "Заблокирован"},
        {"id": "sentinel.table_created_at", "message": "Created At", "translation": "Создан"},
        {"id": "sentinel.no_bans", "message": "No banned IPs", "translation": "Нет заблокированных IP"},
        {"id": "sentinel.no_bans_hint", "message": "Add an IP pattern using the form above to start blocking.", "translation": "Добавьте IP-адрес через форму выше для начала блокировки."},
        {"id": "sentinel.confirm_delete", "message": "Remove this IP ban?", "translation": "Снять эту блокировку?"},
        {"id": "sentinel.delete_failed", "message": "Failed to remove", "translation": "Не удалось удалить"},
        {"id": "sentinel.success_created", "message": "IP has been banned", "translation": "IP-адрес заблокирован"},
        {"id": "sentinel.error_ip_required", "message": "IP pattern is required", "translation": "IP-адрес обязателен"},
        {"id": "sentinel.error_invalid_pattern", "message": "Invalid IP pattern format", "translation": "Неверный формат IP-адреса"},
        {"id": "sentinel.error_duplicate", "message": "This IP pattern is already banned", "translation": "Этот IP-адрес уже заблокирован"},
        {"id": "sentinel.error_create_failed", "message": "Failed to create ban", "translation": "Не удалось создать блокировку"},
        {"id": "sentinel.error_self_ban", "message": "Cannot ban your own IP address", "translation": "Нельзя заблокировать собственный IP-адрес"},
        {"id": "sentinel.access_denied", "message": "Access denied", "translation": "Доступ запрещен"}
    ]
}
